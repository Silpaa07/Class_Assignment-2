//Write a C program to add all the elements of an array.
#include<stdio.h>
int main()
{
    int n,sum=0;
    printf("Enter the number of array elements:\n");
    scanf("%d",&n);
    int arr[n];
    printf("Enter the array elements:\n");
    for(int i=0;i<n;i++)
    {
        scanf("%d",&arr[i]);
    }
    for(int i=0;i<n;i++)
    {
        sum=sum+arr[i];
    }
    printf("The sum of array elements:%d",sum);
}
